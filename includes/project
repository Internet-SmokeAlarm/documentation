        <span class="s">'ID'</span><span class="p">:</span> <span class="s">'cdace1ad391e444aa5ade2ce7f04c5e6'</span><span class="p">,</span>
        <span class="s">'devices'</span><span class="p">:</span> <span class="p">{},</span>
        <span class="s">'experiments'</span><span class="p">:</span> <span class="p">{</span>
